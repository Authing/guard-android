<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="textColor" format="reference|color" />
    <attr name="text" format="reference|string" />
    <attr name="textSize" format="dimension" />
    <attr name="type" format="enum" >
        <enum name="phoneCode" value="0"/>
        <enum name="accountPassword" value="1"/>
        <enum name="phoneCodePassword" value="2"/>
        <enum name="emailPassword" value="3"/>
        <enum name="emailCode" value="4"/>
    </attr>

    <attr name="pageType" format="enum" >
        <enum name="login" value="0"/>
        <enum name="register" value="1"/>
    </attr>

    <attr name="itemGravity" format="enum">
        <enum name="left" value="0" />
        <enum name="start" value="0" />
        <enum name="center" value="1" />
        <enum name="right" value="2" />
        <enum name="end" value="2" />
    </attr>

    <declare-styleable name="LoadingButton">
        <attr name="android:tint" format="reference|color" />
        <attr name="loadingLocation" format="enum">
            <enum name="left" value="0"/>
            <enum name="right" value="1"/>
            <enum name="over" value="2"/>
            <enum name="cover" value="3"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="LoginMethodTab">
        <attr name="itemGravity" />
    </declare-styleable>

    <declare-styleable name="LoginContainer">
        <attr name="type" />
    </declare-styleable>

    <declare-styleable name="RegisterMethodTab">
        <attr name="itemGravity" />
    </declare-styleable>

    <declare-styleable name="RegisterContainer">
        <attr name="type" />
    </declare-styleable>

    <declare-styleable name="EditTextLayout">
        <attr name="pageType" />
        <attr name="inputHeight" format="dimension"/>
        <attr name="errorHeight" format="dimension"/>
        <attr name="leftIconDrawable" format="reference" />
        <attr name="hintMode" format="enum">
            <enum name="normal" value="0"/>
            <enum name="animated" value="1"/>
        </attr>
        <attr name="clearAllEnabled" format="boolean" />
        <attr name="android:textSize" />
        <attr name="android:textColor" />
        <attr name="android:hint" format="string" />
        <attr name="android:lines" format="integer" />
        <attr name="android:maxLines" format="integer" />
        <attr name="android:singleLine" format="boolean" />
        <attr name="android:inputType" format="enum" />
        <attr name="errorTextSize" format="dimension"/>
        <attr name="hintColor" format="reference|color" />
        <attr name="errorEnabled" format="boolean" />
        <attr name="enabled" format="boolean" />
    </declare-styleable>

    <declare-styleable name="BasePasswordEditText">
        <attr name="toggleEnabled" format="boolean" />
    </declare-styleable>

    <declare-styleable name="PhoneNumberEditText">
        <attr name="dividerPattern" format="integer" />
    </declare-styleable>

    <declare-styleable name="VerifyCodeEditText">
        <attr name="codeMode" format="enum">
            <enum name="normal" value="0"/>
            <enum name="frame" value="1"/>
            <enum name="underLine" value="2"/>
        </attr>
        <attr name="boxWidth" format="dimension" />
        <attr name="boxHeight" format="dimension" />
        <attr name="boxSpacing" format="dimension" />
        <attr name="digit" format="integer" />
        <attr name="hyphen" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SocialLoginListView">
        <attr name="loginSource" format="string" />
    </declare-styleable>

    <declare-styleable name="CountryCodePicker">
        <attr name="showFlag" format="boolean" />
        <attr name="showCountryName" format="boolean" />
        <attr name="showRightArrow" format="boolean" />
    </declare-styleable>

    <declare-styleable name="PrivacyConfirmBox">
        <attr name="android:textColor" />
        <attr name="android:text" />
        <attr name="android:textSize" />
        <attr name="button" format="reference" />
        <attr name="linkTextColor" format="reference|color" />
        <attr name="uncheckedColor" format="reference|color" />
        <attr name="checkedColor" format="reference|color" />
        <attr name="isRound" format="boolean" />
        <attr name="isShowInternal" format="boolean" />
        <attr name="isRememberSate" format="boolean" />
        <attr name="dialogMessage" format="reference|string" />
    </declare-styleable>

    <declare-styleable name="MandatoryField">
        <attr name="asteriskPosition" format="enum">
            <enum name="none" value="0"/>
            <enum name="right" value="1"/>
            <enum name="left" value="2"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="MaskLabel">
        <attr name="maskLength" format="integer" />
    </declare-styleable>

    <declare-styleable name="RecoveryCodeEditText">
        <attr name="copyEnabled" format="boolean" />
    </declare-styleable>

    <declare-styleable name="AuthContainer">
        <attr name="authProtocol" format="enum" >
            <enum name="inHouse" value="0"/>
            <enum name="OIDC" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="OneClickAuthButton">
        <attr name="showByConfig" format="boolean" />
        <attr name="showLoading" format="boolean" />
    </declare-styleable>

    <declare-styleable name="GetVerifyCodeButton">
        <attr name="verifyCodeType" format="enum" >
            <enum name="none" value="0"/>
            <enum name="phone" value="1"/>
            <enum name="email" value="2"/>
        </attr>
        <attr name="verifyAuthType" format="enum" >
            <enum name="none" value="0"/>
            <enum name="login" value="1"/>
            <enum name="register" value="2"/>
            <enum name="resetPassword" value="3"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="TitleLayout">
        <attr name="showBackIcon" format="boolean" />
        <attr name="showTitleText" format="boolean" />
        <attr name="showSkipText" format="boolean" />
        <attr name="showSkipIcon" format="boolean" />
        <attr name="titleText" format="string" />
        <attr name="titleTextColor" format="reference|color" />
        <attr name="titleTextSize" format="dimension" />
        <attr name="titleTextBold" format="boolean" />
        <attr name="skipText" format="string" />
        <attr name="skipTextColor" format="reference|color" />
        <attr name="skipTextSize" format="dimension" />
        <attr name="skipTextBold" format="boolean" />
        <attr name="checkNetWork" format="boolean" />
        <attr name="onlyCheckNetWork" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ContinueWithTextView">
        <attr name="middleText" format="string" />
        <attr name="middleTextColor" format="reference|color"/>
        <attr name="middleTextSize" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="GoLoginTipsText">
        <attr name="countDownTime" format="integer" />
    </declare-styleable>

    <declare-styleable name="FeedbackDescriptionEditText">
        <attr name="android:textColor" />
        <attr name="android:textSize" />
        <attr name="android:hint" format="string" />
        <attr name="android:lines" format="integer" />
        <attr name="android:maxLines" format="integer" />
        <attr name="android:singleLine" format="boolean" />
        <attr name="hintTextColor" format="reference|color" />
        <attr name="maxNumberOfWords" format="string" />
    </declare-styleable>

    <declare-styleable name="LetterSideBar">
        <attr name="letterTextArray" format="reference" />
        <attr name="letterTextColor" format="color" />
        <attr name="letterTextSelectColor" format="color" />
        <attr name="letterTextSize" format="dimension" />
        <attr name="letterBackground" format="reference|color" />
        <attr name="showTextDialog" format="boolean" />
        <attr name="dialogTextColor" format="color" />
        <attr name="dialogTextSize" format="dimension" />
        <attr name="dialogTextBackground" format="reference|color" />
        <attr name="dialogTextBackgroundWidth" format="dimension" />
        <attr name="dialogTextBackgroundHeight" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="circleImageRadius" format="dimension"/>
    </declare-styleable>

</resources>